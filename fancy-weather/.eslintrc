{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "extends": "airbnb-base",
  "rules": {
    "no-console": "off",
    "guard-for-in": 0,
    "no-restricted-syntax": 0,
    "import/prefer-default-export": "off",
    "no-underscore-dangle": 0,
    "class-methods-use-this": 0,
    "no-param-reassign": 0,
    "no-alert": 0,
    "no-undef": 0
    }
  }
